{
  "browser": {
    "browserName": "chromium",
    "launchOptions": {
      "channel": "chrome",
      "headless": true
    },
    "contextOptions": {
      "viewport": {
        "width": 1440,
        "height": 900
      }
    }
  },
  "capabilities": [
    "core",
    "tabs",
    "pdf",
    "history",
    "wait",
    "files"
  ],
  "vision": false,
  "outputDir": "./temp",
  "tools": {
    "browser_take_screenshot": {
      "omitBase64": false
    }
  }
}